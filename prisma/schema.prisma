datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String  @id @default(uuid())
  name       String
  github_id  Int     @unique
  avatar_url String
  login      String  @unique
  password   String?

  @@map("users")
}
